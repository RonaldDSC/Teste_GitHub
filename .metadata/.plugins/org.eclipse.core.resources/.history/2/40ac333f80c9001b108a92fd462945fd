import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int N;
		int soma = 0;
		
		System.out.println("quantas vezas voce quer repitir? ");
		N = sc.nextInt();
		
		for(int i=0; i<N; i =+ 1) {
			int x = sc.nextInt();
			soma = soma + x;		
		}
		
		System.out.println(soma);
		
		sc.close();
	}
}
